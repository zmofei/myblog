extends ../layout.en.jade

block title
    title Lab - I'm Mofei

block css
    link(rel="stylesheet" href= host + "/css/lab.css")

block javascript
    script(src= host + "/js/tools/utility.js")
    script(src= host + "/js/lab/lab.main.js")

block content    
    include ../common/nav.en.jade
    section.lab
        div.lab-box
            div.lab-block-addone
                div.lab-block
                    a(href="https://github.com/huiyan-fe/mapv" target="_blank")
                        img(src= host + "/image/lab/mapv.jpg")
                    .lab-block-content
                        div.lab-block-title 
                            a(href="https://github.com/huiyan-fe/mapv" target="_blank") Mapv    
                        div.lab-block-intro The geographic data visualization open source library.
                        div.lab-block-author Author By
                            a(href="http://nikai.us/" target="_blank") 倪楷
                            span as first author And
                            a(href="https://www.zhuwenlong.com" target="_blank") Mofei
                            span as co-first author
                div.lab-block
                    a(href="https://zmofei.github.io/jDate/demo/" target="_blank")
                        img(src= host + "/image/lab/jDate.jpg")
                    .lab-block-content
                        div.lab-block-title 
                            a(href="/lab/mstaticize" target="_blank") jDate
                        div.lab-block-intro ..makes DATE & TIME choose easy that your think。
                        div.lab-block-author Author By
                            a(href="https://www.zhuwenlong.com" target="_blank") Mofei
                            span as first author
                div.lab-block
                    a(href="/lab/mstaticize" target="_blank")
                        img(src= host + "/image/lab/mstaticize.jpg")
                    .lab-block-content
                        div.lab-block-title 
                            a(href="/lab/mstaticize" target="_blank") mStaticize
                        div.lab-block-intro ☺ Simple configuration, to implement the static (hash) of front-end resources, automatic substitution of resource references.
                        div.lab-block-author Author By
                            a(href="https://www.zhuwenlong.com" target="_blank") Mofei
                            span as first author
            div.lab-block-addone
                div.lab-block
                    a(href="https://github.com/zmofei/dufing" target="_blank")
                        img(src= host + "/image/lab/dufing.jpg")
                    .lab-block-content
                        div.lab-block-title 
                            a(href="https://github.com/zmofei/dufing" target="_blank") Dufing
                        div.lab-block-intro A lightweight, file-based node.js web server framework.
                        div.lab-block-author Author By
                            a(href="https://www.zhuwenlong.com" target="_blank") Mofei
                            span as first author
            
    section.index-copyright
        div.index-copyright-bottom
            include ../copyright.en.jade
